<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_form_category" model="ir.ui.view">
            <field name="name">product.category.code</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view" />
            
            <field name="arch" type="xml">
                <xpath expr="//field[@name='parent_id']" position="after">
                    <field name="correlacion" placeholder="Correlacion de referencia"/>
                    <field name="correlacion_maxima" placeholder="Correlacion de referencia maxima"/>
                    
                </xpath>
            </field>
        </record>
        
    </data>
</odoo>
