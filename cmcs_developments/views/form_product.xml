<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="form_product_template" model="ir.ui.view">
            <field name="name">product.template.form.client.reference</field>
            <field name="model">product.template</field>
             <field name="inherit_id" ref="product.product_template_form_view" />
            <field name="arch" type="xml">
                <field name='detailed_type' position="before"  >
                    <field name='referencia_de_cliente_id'/>
                </field>
                
                <field name='description_sale' position="after"  >
                    <field name='venta'/>
                </field>
                
            </field>
        </record>
    </data>
</odoo>
